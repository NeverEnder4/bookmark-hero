{
  "name": "Bookmark Hero",
  "description": "The bookmark manager that saves the day.",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "./logo-16.png",
    "32": "./logo-32.png",
    "48": "./logo-48.png",
    "124": "./logo-124.png"
  },
  "background": {
    "scripts": ["./background.js"]
  },
  "options_page": "./options/options.html",
  "browser_action": {
    "default_popup": "./popup/popup.html"
  },
  "permissions": [
    "tabs",
    "bookmarks"
  ]
}